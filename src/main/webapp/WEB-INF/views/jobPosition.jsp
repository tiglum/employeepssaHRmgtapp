<body>
<form action="">
		<br>
		<br><br>
                

		<center>
			<h1>Job Position</h1>
		</center>
		<br>
		<table align="center">


			<tr>
				<th align="right"><strong>Employee ID:</strong></th>
				<td><input name="id" type="text" size="40"></td>
			</tr>
			<tr>
			<tr>
				<th align="right"><strong>Job Title:</strong></th>
				<td><input name="jobTitle" type="text" size="40"></td>
			</tr>
			<tr>
			<tr>
				<th align="right"><strong>Job Level:</strong></th>
				<td><input name="jobLevel" type="text" size="40"></td>
			</tr>
			<tr>
			<tr>
				<th align="right"><strong>Job Type:</strong></th>
				<td><input name="jobType" type="text" size="40"></td>
			</tr>
			<tr>
			<tr>
				<th align="right"><strong>Salary:</strong></th>
				<td><input name="salary" type="text" size="40"></td>
			</tr>
			<tr>
			<tr>
				<th align="right"><strong>Education Level:</strong></th>
				<td><input name="educationLevel" type="text" size="40"></td>
			</tr>
			
