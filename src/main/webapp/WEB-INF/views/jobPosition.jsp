				<th align="right"><strong>Education Type:</strong></th>
				<td><input name="educationType" type="text" size="40"></td>
			</tr>
			<tr>
				<th align="right"><strong>Experience:</strong></th>
				<td><input name="experience" type="text" size="40"></td>
			</tr>
			<tr>
				<th align="right"><strong>Type of Position:</strong></th>
				<td><input name="typeOfPosition" type="text" size="40"></td>
			</tr>
			<tr>
				<th align="right"><strong>Is hired?:</strong></th>
				<td><input name="isHired" type="text" size="40"></td>
			</tr>
			<tr>
				<th align="right"></th>
				<td align="right"><input type="submit" value="Register">
				</td>


			</tr>
			
		</table>


	</form>
